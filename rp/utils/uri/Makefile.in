# $Id$

NAME = uri

BIN = ${NAME}
SRC = ${NAME}.c
OBJ = ${NAME}.o

CFLAGS = @CFLAGS@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@

abs_top_srcdir	 = @abs_top_srcdir@
abs_top_builddir = @abs_top_builddir@

all: ${BIN}

clean:
	rm -rf ${BIN} ${OBJ} ${BIN}.dSYM

${BIN}: ${SRC}
	${CC} ${CFLAGS} -o $@ ${SRC} ${LDFLAGS} ${LIBS}

test:
	@true

install deinstall uninstall:
	@true

distclean: clean
	rm -f Makefile

# $Id$

NAME = uri

BIN = ${NAME}
SRC = ${NAME}.c
OBJ = ${NAME}.o

CFLAGS = @CFLAGS@
LDFLAGS = @LDFLAGS@
LIBS = @LIBS@

all: ${BIN}

clean:
	rm -f ${BIN} ${OBJ}

${BIN}: ${SRC}
	${CC} ${CFLAGS} -o $@ ${SRC} ${LDFLAGS} -lcrypto ${LIBS}

test:
	@true

install:
	@true

distclean: clean
	rm -f Makefile
